{
  "name": "@tinymce/miniature",
  "version": "2.2.1",
  "description": "This library checks versions of TinyMCE.",
  "scripts": {
    "prepublishOnly": "npm run lint && npm run build",
    "lint": "tslint src/**/*.ts",
    "build": "tsc",
    "test": "bedrock-auto -b phantomjs -d src/test/ts/browser",
    "test-manual": "bedrock -d src/test/ts/browser",
    "demo": "parcel serve src/demo/html/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tinymce/miniature"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [],
  "author": "Tiny Technologies",
  "license": "Apache-2.0",
  "dependencies": {
    "@ephox/agar": "^4.15.2",
    "@ephox/dom-globals": "^1.1.2",
    "@ephox/katamari": "^5.0.1",
    "@ephox/mcagar": "^4.1.2",
    "@ephox/sugar": "^5.1.2"
  },
  "devDependencies": {
    "@ephox/bedrock-client": "^9.3.2",
    "@ephox/bedrock-server": "^9.3.2",
    "@ephox/tslint-rules": "^1.0.6",
    "parcel": "^1.10.3",
    "tinymce": "^5.2.0",
    "tinymce-4.5.x": "npm:tinymce@4.5.x",
    "tinymce-4.8.x": "npm:tinymce@4.8.x",
    "tinymce-5.0.x": "npm:tinymce@5.0.x",
    "tslint": "^5.11.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "lib/main",
    "lib/demo",
    "lib/test",
    "src",
    "tsconfig.json",
    "readme.md",
    "LEGAL.txt",
    "CHANGELOG.txt",
    "LICENSE.txt"
  ],
  "main": "./lib/main/ts/api/Main.js",
  "module": "./lib/main/ts/api/Main.js",
  "types": "./lib/main/ts/api/Main.d.ts"
}
